<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.edgedo.sys.mapper.BigdataConfigLineMapper" >
   
   
    <resultMap id="BaseResultMap" type="com.edgedo.sys.entity.BigdataConfigLine" >
			<id column="ID" property="id"/>
			<result column="CREATE_TIME" property="createTime"/>
			<result column="LINE_NAME" property="lineName"/>
			<result column="LINE_KEY" property="lineKey"/>
			<result column="LINE_VALUE" property="lineValue"/>
			<result column="REMARK" property="remark"/>
    </resultMap>
   
    <resultMap id="BaseResultViewMap" type="com.edgedo.sys.queryvo.BigdataConfigLineView" extends="BaseResultMap">
    
    </resultMap>
  
  
  <!-- ******* 新增字段请修改这里  ******  -->
  <sql id="Base_Column_List">
	 ID, CREATE_TIME, LINE_NAME, LINE_KEY, LINE_VALUE, REMARK
  </sql>
  
  <!-- ******* 利用好这里的动态查询条件  ******  -->
  <sql id="where">
  	<where>
				<if test='queryObj != null and  queryObj.createTime != null'>
					AND  CREATE_TIME = #{queryObj.createTime}
				</if>
  	</where>
  	
  </sql>
  
  <!-- ******* 排序判断  ******  
  <sql id="orderBy">
  	<if test="orderBy != null and orderBy!= '' ">
  		order by  ${orderBy}
  	</if>
  </sql>-->
  
  
  <!-- 分页条件查询  -->
  <select id="listPage" parameterType="com.edgedo.sys.queryvo.BigdataConfigLineQuery" resultMap="BaseResultViewMap">
  		select <include refid="Base_Column_List"/> from bigdata_config_line 
  		<include refid="where"/>
  </select>
  
   <!-- 不分页条件查询  -->
  <select id="listByObj" parameterType="com.edgedo.sys.queryvo.BigdataConfigLineQuery" resultMap="BaseResultViewMap">
  		select <include refid="Base_Column_List"/> from bigdata_config_line 
  		<include refid="where"/>
  </select>
  
  
</mapper>