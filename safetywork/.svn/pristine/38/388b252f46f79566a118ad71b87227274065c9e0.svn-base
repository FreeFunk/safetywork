.container00 {


  margin:0px auto;
  border-radius: 0.2rem;
  counter-reset: pagination;
  text-align: center;
}
.container00:after {
  clear: both;
  content: "";
  display: table;
}
.container00 ul {
  width: 100%;
}


.pagination dl, dd {
  list-style: none;
  display: inline;
  padding-left: 0px;
}

.pagination dd {
  counter-increment: pagination;
}
.pagination dd:hover a {
  color: #fdfdfd;
  background-color: #093C5B;
  border: solid 1px #275874;
}
.pagination dd.active a {
  color: #fdfdfd;
  background-color: #093C5B;
  border: solid 1px #275874;
}

.pagination dd:first-child a:after {
  content: "<";
}

.pagination dd:nth-child(2) {
  counter-reset: pagination;
}

.pagination dd:last-child a:after {
  content: ">";
}
.pagination dd a {
  border: solid 1px #275874;
  border-radius: 0.2rem;
  color: #7d7d7d;
  text-decoration: none;
  text-transform: uppercase;
  display: inline-block;
  text-align: center;
  padding: 0.5rem 0.9rem;
}
.pagination dd a:after {
  content: " " counter(pagination) " ";
}

.large dd a {
  display: none;
}
.large dd:first-child a {
  display: inline-block;
}
.large dd:first-child a:after {
  content: "<";
}
.large dd:nth-child(2) a {
  display: inline-block;
}
.large dd:nth-child(3) a {
  display: inline-block;
}
.large dd:nth-child(4) a {
  display: inline-block;
}
.large dd:nth-child(5) a {
  display: inline-block;
}
.large dd:nth-child(6) a {
  display: inline-block;
}
.large dd:nth-child(7) a {
  display: inline-block;
}
.large dd:nth-child(8) a {
  display: inline-block;
}
.large dd:last-child a {
  display: inline-block;
}
.large dd:last-child a:after {
  content: ">";
}
.large dd:nth-last-child(2) a {
  display: inline-block;
}
.large dd:nth-last-child(3) {
  display: inline-block;
}
.large dd:nth-last-child(3):after {
  padding: 0 1rem;
  content: "...";
  color:#fff
}


