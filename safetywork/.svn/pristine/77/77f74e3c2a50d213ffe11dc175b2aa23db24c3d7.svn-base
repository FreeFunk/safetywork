<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>标题</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="../../../../../layuiadmin/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../../../../../layuiadmin/style/admin.css" media="all">
</head>
<body>

<div class="layui-fluid">
    <div class="layui-card">
        <div class="layui-form layui-card-header layuiadmin-card-header-auto">
            <div class="layui-form-item">
                <div class="layui-inline">
                    <label class="layui-form-label">搜索条件1</label>
                    <div class="layui-input-inline">
                        <input type="text" name="queryObj.name" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <label class="layui-form-label">搜索条件2</label>
                    <div class="layui-input-inline">
                        <input type="text" name="queryObj.name" placeholder="请输入" autocomplete="off" class="layui-input">
                    </div>
                </div>
                <div class="layui-inline">
                    <button class="layui-btn layuiadmin-btn-comm" data-type="reload" lay-submit lay-filter="BigdataBeidouCompMonthCheck-search">
                        <i class="layui-icon layui-icon-search layuiadmin-button-btn"></i>
                    </button>
                </div>
            </div>
        </div>
        <div class="layui-card-body">
            <div style="padding-bottom: 10px;">
                <button class="layui-btn layuiadmin-btn-comm" data-type="batchdel">删除</button>
                <button class="layui-btn layuiadmin-btn-comm" data-type="add">添加</button>
            </div>
            <table id="BigdataBeidouCompMonthCheck-list" lay-filter="BigdataBeidouCompMonthCheck-list"></table>
            <script type="text/html" id="table-BigdataBeidouCompMonthCheck-list">
                <a class="layui-btn layui-btn-normal layui-btn-xs" lay-event="edit"><i class="layui-icon layui-icon-edit"></i>编辑</a>
                <a class="layui-btn layui-btn-danger layui-btn-xs" lay-event="del"><i class="layui-icon layui-icon-delete"></i>删除</a>
            </script>
        </div>
    </div>
</div>
<script src="../../../../../layuiadmin/layui/layui.js"></script>
<script>
        layui.config({
            base: '../../../../../layuiadmin/' //静态资源所在路径
        }).extend({
            index:'lib/index',
            common:'common'
        }).use(['index','table', 'form','common'], function(exports){
            var $ = layui.$
                ,table = layui.table
                ,common = layui.common
                ,admin = layui.admin
                ,form = layui.form;

            //查询
            form.on('submit(BigdataBeidouCompMonthCheck-search)', function(data){
                var field = data.field;

                //执行重载
                table.reload('BigdataBeidouCompMonthCheck-list', {
                    where: field
                });
            });

            /*
            * 定义表格
            * */
            defineTable = function () {
                var headers = layui.admin.getTokenHeader();
                table.render({
                    elem: '#BigdataBeidouCompMonthCheck-list' //指定原始表格的选择器（id选择器）
                    ,url:layui.setter.basePath +'/admin/bigdataBeidouCompMonthCheck/listpage.jsn' //模拟接口
                    ,headers:headers
                    ,method: 'post'
                    ,parseData: function(res){ //res 即为原始返回的数据
                        return {
                            "code": res.success, //解析接口状态
                            "msg": res.errMsg, //解析提示文本
                            "count": res.totalCount, //解析数据长度
                            "data": res.list //解析数据列表
                        };
                    }
                    ,response: {
                        statusCode: true //规定成功的状态码，默认：0
                    }
                    ,cols: [[
                        {type: 'checkbox', fixed: 'left'}
                    ,{field: 'id',align: 'center', width: 200, title: '主键',sort: true,hide:true}
                    ,{field: 'createTime',align: 'center', width: 200, title: '创建时间',templet: function(d){
                    return common.DateFormat(d.createTime,"yyyy-MM-dd");},sort: true}
                    ,{field: 'updateTime',align: 'center', width: 200, title: '更新时间',templet: function(d){
                    return common.DateFormat(d.updateTime,"yyyy-MM-dd");},sort: true}
            ,{field: 'compId',align: 'center', width: 200, title: '运营商id',sort: true}
            ,{field: 'compName',align: 'center', width: 200, title: '运营商名',sort: true}
            ,{field: 'score',align: 'center', width: 200, title: '得分',sort: true}
            ,{field: 'carNum',align: 'center', width: 200, title: '车辆总数',sort: true}
            ,{field: 'onlineCarNum',align: 'center', width: 200, title: '当月上线数',sort: true}
            ,{field: 'onlineRate',align: 'center', width: 200, title: '上线率',sort: true}
            ,{field: 'linkBreakMinute',align: 'center', width: 200, title: '当月中断分钟数',sort: true}
            ,{field: 'linkRate',align: 'center', width: 200, title: '联通率',sort: true}
            ,{field: 'traceUncompleteCarNum',align: 'center', width: 200, title: '当月轨迹不完整车数',sort: true}
            ,{field: 'traceCompleteRate',align: 'center', width: 200, title: '轨迹完整率',sort: true}
            ,{field: 'dataQualifiedRate',align: 'center', width: 200, title: '数据合格率',sort: true}
            ,{field: 'driverCardUpCarNum',align: 'center', width: 200, title: '司机卡当月上传数',sort: true}
            ,{field: 'driverCardUploadRate',align: 'center', width: 200, title: '司机卡上传率',sort: true}
            ,{field: 'driverCarRecCount',align: 'center', width: 200, title: '司机卡记录总数',sort: true}
            ,{field: 'driverCardQualifiedNum',align: 'center', width: 200, title: '司机卡合格数',sort: true}
            ,{field: 'driverCardQualifiedRate',align: 'center', width: 200, title: '司机身份卡信息准确率',sort: true}
            ,{field: 'speedAlarmQualifiedRate',align: 'center', width: 200, title: '分段限速报警准确率',sort: true}
            ,{field: 'traceFlayCarNum',align: 'center', width: 200, title: '轨迹漂移车辆数',sort: true}
            ,{field: 'traceFlyRate',align: 'center', width: 200, title: '车辆漂移率',sort: true}
            ,{field: 'gpsUploadTimeQualifiedNum',align: 'center', width: 200, title: '回传间隔合格车辆数',sort: true}
            ,{field: 'gpsUploadTimeQualifiedRate',align: 'center', width: 200, title: '回传间隔合格率',sort: true}
            ,{field: 'countMonth',align: 'center', width: 200, title: '统计月份',sort: true}
            ,{field: 'yearNum',align: 'center', width: 200, title: '年份',sort: true}
                    ,{title: '操作', minWidth: 150, align: 'center', fixed: 'right', toolbar: '#table-BigdataBeidouCompMonthCheck-list'}
            ]]
            ,autoSort: false
                    ,page: true
                    ,limit: 10
                    ,limits: [10, 20, 30,40]
                    ,text: {
                    none: '暂无相关数据'
                }
            });

                //为toolbar添加事件响应
                table.on('tool(BigdataBeidouCompMonthCheck-list)', function(obj){ //注：tool是工具条事件名，test是table原始容器的属性 lay-filter="对应的值"
                    /*var data = obj.data;*///获得当前行数据
                    //区分事件
                    switch(obj.event){
                        case 'del':
                            deleteVo(obj);
                            break;
                        case 'edit':
                            editVo(obj);
                            break;
                    };
                });

                //监听排序事件
                table.on('sort(BigdataBeidouCompMonthCheck-list)', function(obj){ //注：sort是工具条事件名，LAY-app-province-list是table原始容器的属性 lay-filter="对应的值"
                    var sort = obj.field;
                    //默认排序
                    switch(obj.field){
                        case 'id':
                            sort ="ID";
                            break;
                        case 'createTime':
                            sort ="CREATE_TIME";
                            break;
                        case 'updateTime':
                            sort ="UPDATE_TIME";
                            break;
                        case 'compId':
                            sort ="COMP_ID";
                            break;
                        case 'compName':
                            sort ="COMP_NAME";
                            break;
                        case 'score':
                            sort ="SCORE";
                            break;
                        case 'carNum':
                            sort ="CAR_NUM";
                            break;
                        case 'onlineCarNum':
                            sort ="ONLINE_CAR_NUM";
                            break;
                        case 'onlineRate':
                            sort ="ONLINE_RATE";
                            break;
                        case 'linkBreakMinute':
                            sort ="LINK_BREAK_MINUTE";
                            break;
                        case 'linkRate':
                            sort ="LINK_RATE";
                            break;
                        case 'traceUncompleteCarNum':
                            sort ="TRACE_UNCOMPLETE_CAR_NUM";
                            break;
                        case 'traceCompleteRate':
                            sort ="TRACE_COMPLETE_RATE";
                            break;
                        case 'dataQualifiedRate':
                            sort ="DATA_QUALIFIED_RATE";
                            break;
                        case 'driverCardUpCarNum':
                            sort ="DRIVER_CARD_UP_CAR_NUM";
                            break;
                        case 'driverCardUploadRate':
                            sort ="DRIVER_CARD_UPLOAD_RATE";
                            break;
                        case 'driverCarRecCount':
                            sort ="DRIVER_CAR_REC_COUNT";
                            break;
                        case 'driverCardQualifiedNum':
                            sort ="DRIVER_CARD_QUALIFIED_NUM";
                            break;
                        case 'driverCardQualifiedRate':
                            sort ="DRIVER_CARD_QUALIFIED_RATE";
                            break;
                        case 'speedAlarmQualifiedRate':
                            sort ="SPEED_ALARM_QUALIFIED_RATE";
                            break;
                        case 'traceFlayCarNum':
                            sort ="TRACE_FLAY_CAR_NUM";
                            break;
                        case 'traceFlyRate':
                            sort ="TRACE_FLY_RATE";
                            break;
                        case 'gpsUploadTimeQualifiedNum':
                            sort ="GPS_UPLOAD_TIME_QUALIFIED_NUM";
                            break;
                        case 'gpsUploadTimeQualifiedRate':
                            sort ="GPS_UPLOAD_TIME_QUALIFIED_RATE";
                            break;
                        case 'countMonth':
                            sort ="COUNT_MONTH";
                            break;
                        case 'yearNum':
                            sort ="YEAR_NUM";
                            break;
                    };
                    if(obj.type==null){
                        return;
                    }
                    //有些时候，你可能需要根据当前排序的字段，重新向服务端发送请求，从而实现服务端排序，如：
                    table.reload('BigdataBeidouCompMonthCheck-list', {
                        initSort: obj//记录初始排序，如果不设的话，将无法标记表头的排序状态。
                        ,where: { //请求参数（注意：这里面的参数可任意定义，并非下面固定的格式）
                            orderBy: sort+" "+obj.type //排序字段
                        }
                    });
                });
            }
            defineTable();

            //删除
            deleteVo = function (obj) {
                layer.confirm('确定删除此记录吗？', function(index){
                    admin.req({
                        url:layui.setter.basePath +'/admin/bigdataBeidouCompMonthCheck/deleteByIds.jsn',
                        data:{'ids':obj.data.id},
                        method:"post",
                        success:function (data) {
                            table.reload('BigdataBeidouCompMonthCheck-list',
                                {page:
                                    { curr: common.deleteJumpPage(obj) // 执行页面跳转方法
                                    }
                                }
                            );
                            layer.close(index);
                            layer.msg('删除成功', {
                                icon: 1
                            });
                        },
                    });
                });
            }
            //编辑
            editVo = function (obj) {
                layer.open({
                    type: 2
                    ,title: '编辑'
                    ,content: 'BigdataBeidouCompMonthCheckEdit.html'
                    ,maxmin: true
                    ,area: ['550px', '550px']
                    ,btn: ['确定', '取消']
                    ,success: function(layero, index){
                        var s = layero.find('iframe');
                        $("id",s[0].contentWindow.document).val(obj.data.id);
                        //给iframe元素赋值
                        var othis = layero.find('iframe').contents().find("#BigdataBeidouCompMonthCheck-form").click();

                        othis.find('input[name="id"]').val(obj.data.id);
                        othis.find('input[name="createTime"]').val(obj.data.createTime);
                        othis.find('input[name="updateTime"]').val(obj.data.updateTime);
                        othis.find('input[name="compId"]').val(obj.data.compId);
                        othis.find('input[name="compName"]').val(obj.data.compName);
                        othis.find('input[name="score"]').val(obj.data.score);
                        othis.find('input[name="carNum"]').val(obj.data.carNum);
                        othis.find('input[name="onlineCarNum"]').val(obj.data.onlineCarNum);
                        othis.find('input[name="onlineRate"]').val(obj.data.onlineRate);
                        othis.find('input[name="linkBreakMinute"]').val(obj.data.linkBreakMinute);
                        othis.find('input[name="linkRate"]').val(obj.data.linkRate);
                        othis.find('input[name="traceUncompleteCarNum"]').val(obj.data.traceUncompleteCarNum);
                        othis.find('input[name="traceCompleteRate"]').val(obj.data.traceCompleteRate);
                        othis.find('input[name="dataQualifiedRate"]').val(obj.data.dataQualifiedRate);
                        othis.find('input[name="driverCardUpCarNum"]').val(obj.data.driverCardUpCarNum);
                        othis.find('input[name="driverCardUploadRate"]').val(obj.data.driverCardUploadRate);
                        othis.find('input[name="driverCarRecCount"]').val(obj.data.driverCarRecCount);
                        othis.find('input[name="driverCardQualifiedNum"]').val(obj.data.driverCardQualifiedNum);
                        othis.find('input[name="driverCardQualifiedRate"]').val(obj.data.driverCardQualifiedRate);
                        othis.find('input[name="speedAlarmQualifiedRate"]').val(obj.data.speedAlarmQualifiedRate);
                        othis.find('input[name="traceFlayCarNum"]').val(obj.data.traceFlayCarNum);
                        othis.find('input[name="traceFlyRate"]').val(obj.data.traceFlyRate);
                        othis.find('input[name="gpsUploadTimeQualifiedNum"]').val(obj.data.gpsUploadTimeQualifiedNum);
                        othis.find('input[name="gpsUploadTimeQualifiedRate"]').val(obj.data.gpsUploadTimeQualifiedRate);
                        othis.find('input[name="countMonth"]').val(obj.data.countMonth);
                        othis.find('input[name="yearNum"]').val(obj.data.yearNum);
                    }
                    ,yes: function(index, layero){
                        var submit = layero.find('iframe').contents().find("#BigdataBeidouCompMonthCheck-form-edit");
                        submit.trigger('click');
                    }
                });
            }

            //点击事件
            var active = {
                //批量删除
                batchdel: function(){
                    var checkStatus = table.checkStatus('BigdataBeidouCompMonthCheck-list')
                        ,checkData = checkStatus.data; //得到选中的数据

                    if(checkData.length === 0){
                        return layer.msg('请选择数据');
                    }

                    layer.confirm('确定删除吗？', function(index) {
                        var ids = "" ;
                        for(var i = 0;i<checkData.length;i++){
                            if(ids==""){
                                ids= checkData[i].id;
                            }else {
                                ids = ids+","+checkData[i].id;
                            }
                        }
                        admin.req({
                            url:layui.setter.basePath +'/admin/bigdataBeidouCompMonthCheck/deleteByIds.jsn',
                            data:{'ids':ids},
                            method:"post",
                            success:function (data) {
                                table.reload('BigdataBeidouCompMonthCheck-list',
                                    {page:
                                        { curr: common.deleteDulJumpPage(checkStatus) // 执行页面跳转方法
                                        }
                                    }
                                );
                                layer.msg('删除成功', {
                                    icon: 1
                                });
                            },
                        });
                    });
                }
                //添加
                ,add: function(){
                    layer.open({
                        type: 2
                        ,title: '添加'
                        ,content: 'BigdataBeidouCompMonthCheckAdd.html'
                        ,maxmin: true
                        ,area: ['550px', '550px']
                        ,btn: ['确定', '取消']
                        ,yes: function(index, layero){
                            //点击确认触发 iframe 内容中的按钮提交
                            var submit = layero.find('iframe').contents().find("#BigdataBeidouCompMonthCheck-form-submit");
                            submit.click();
                        }
                    });
                }
            }
            //绑定点击事件
            $('.layui-btn.layuiadmin-btn-comm').on('click', function(){
                var type = $(this).data('type');
                active[type] ? active[type].call(this) : '';
            });


        });

</script>
</body>
</html>

